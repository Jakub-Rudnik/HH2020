{
    "parser": "babel-eslint",
    "extends": [
        "airbnb",
        "eslint:recommended",
        "plugin:react/recommended",
        "prettier",
        "prettier/react"
    ],
    "settings": {
        "import/resolver": {
            "node": {
                "paths": ["./src"],
                "extensions": ["*.js"]
            }
        }
    },
    "rules": {
        "react/jsx-filename-extension": 0,
        "react/prop-types": 0
    }
}